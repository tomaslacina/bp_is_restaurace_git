<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bp_is_restaurace.UvodniObrazovkaController">
   <children>
      <Label fx:id="label_prihlaseni" layoutX="112.0" layoutY="31.0" text="Přihlášení do systému proběhlo úspěšně">
         <font>
            <Font name="Arial Black" size="17.0" />
         </font>
      </Label>
      <HBox layoutX="83.0" layoutY="110.0" prefHeight="103.0" prefWidth="414.0">
         <children>
            <VBox prefHeight="103.0" prefWidth="115.0">
               <children>
                  <ImageView fitHeight="103.0" fitWidth="116.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@obrazky/uzivatel.png" />
                     </image>
                  </ImageView>
               </children>
            </VBox>
            <VBox prefHeight="95.0" prefWidth="150.0" style="-fx-background-color: lightgray;">
               <children>
                  <Label prefHeight="32.0" prefWidth="152.0" text="Jméno uživatele:">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font></Label>
                  <Label layoutX="10.0" layoutY="10.0" prefHeight="32.0" prefWidth="152.0" text="Osobní číslo:">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                  </Label>
                  <Label layoutX="10.0" layoutY="42.0" prefHeight="32.0" prefWidth="152.0" text="Restaurace:">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox prefHeight="130.0" prefWidth="150.0" style="-fx-background-color: #FFDDA1;">
               <children>
                  <Label fx:id="label_jmeno" prefHeight="32.0" prefWidth="152.0" textFill="RED">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="label_osobniCislo" layoutX="10.0" layoutY="10.0" prefHeight="32.0" prefWidth="152.0" textFill="RED">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="label_restaurace" layoutX="10.0" layoutY="42.0" prefHeight="32.0" prefWidth="151.0" textFill="RED">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                  </Label>
               </children></VBox>
         </children>
      </HBox>
      <ButtonBar layoutX="83.0" layoutY="280.0" prefHeight="40.0" prefWidth="414.0">
        <buttons>
            <Button fx:id="btn_restaurace" mnemonicParsing="false" prefHeight="40.0" prefWidth="126.0" style="-fx-background-color: green;" text="Restaurace" textFill="WHITE">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btn_administrace" mnemonicParsing="false" prefHeight="42.0" prefWidth="158.0" style="-fx-background-color: blue;" text="Administrace" textFill="WHITE">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btn_odhlasitSe" mnemonicParsing="false" prefHeight="42.0" prefWidth="158.0" style="-fx-background-color: red;" text="Odhlásit se" textFill="WHITE">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
